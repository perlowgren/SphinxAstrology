<?xml version="1.0" encoding="utf-8" ?>
<inset xmlns:android="http://schemas.android.com/apk/res/android"
       android:insetLeft="@dimen/edit_text_inset_horizontal"
       android:insetRight="@dimen/edit_text_inset_horizontal"
       android:insetTop="@dimen/edit_text_inset_top"
       android:insetBottom="@dimen/edit_text_inset_bottom">
  <selector>
    <item android:state_enabled="false"
          android:drawable="@drawable/simple_spinner_disabled" />
    <item android:state_pressed="false"
          android:state_focused="false"
          android:drawable="@drawable/simple_spinner_default" />
    <item android:state_pressed="true"
          android:drawable="@drawable/simple_spinner_pressed" />
    <item android:state_pressed="false"
          android:state_focused="true"
          android:drawable="@drawable/simple_spinner_focused" />
  </selector>
</inset>
